{
    "name": "hifikabin/googlesearch",
    "type": "phpbb-extension",
    "description": "Adds a Google Search to your forum which is a visual match to my NavBar Search Extension 3.1.0. You can also enter your Google Site Verefication code. NOTE This extension is for phpBB3.1.x ONLY",
    "homepage": "http://phpbb.hifikabin.me.uk",
    "version": "3.1.0",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "James Newcombe",
            "username": "HiFiKabin",
            "email": "webmaster@hifikabin.me.uk",
            "homepage": "http://phpbb.hifikabin.me.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.4"
    },
    "extra": {
        "display-name": "Google Search & Site Verification",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.4"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/googlesearch",
            "filename": "version_check"
        }
    }
}